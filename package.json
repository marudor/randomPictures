{
  "name": "randompictures",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "packageManager": "pnpm@7.2.1",
  "scripts": {
    "build:babel": "babel src -x '.ts' --copy-files --out-dir dist",
    "build:watch": "yarn build:babel --watch",
    "build": "run-s clean build:babel",
    "clean": "rimraf dist",
    "dev": "run-p build:watch start:watch",
    "start:watch": "nodemon --watch dist --exec 'node dist/index.js'",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint --ext .ts src/",
    "unsplash": "babel-node -x '.ts' src/util/scrapeUnsplash.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marudor/randomPictures.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marudor/randomPictures/issues"
  },
  "homepage": "https://github.com/marudor/randomPictures#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "pnpm": {
    "overrides": {
      "typescript": "^4.4.4"
    }
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@timberio/node": "^0.35.0",
    "axios": "^0.23.0",
    "cookie": "^0.4.1",
    "core-js": "^3.18.3",
    "easyimage": "^3.1.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "on-finished": "^2.3.0",
    "pino": "^7.0.3",
    "pino-pretty": "^7.1.0",
    "pino-std-serializers": "^4.0.0",
    "prom-client": "^14.0.0",
    "random-js": "^2.1.0",
    "twit": "^2.2.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/archiver": "^5.1.1",
    "@types/cookie": "^0.4.1",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "8.0.11",
    "@types/node": "^16.11.2",
