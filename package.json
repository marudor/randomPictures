    "@types/twit": "^2.2.30",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-marudor": "^8.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24",
    "giphy-api": "^2.0.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
